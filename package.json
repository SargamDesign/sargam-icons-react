{
  "name": "@sargamdesign/icons-react",
  "version": "1.6.2",
  "description": "A collection of open-source icons crafted to match your exquisite designs.",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "bunx rimraf dist",
    "build": "bun run line && bun run duotone && bun run fill && bunx rollup -c --bundleConfigAsCjs && bun run gen:types",
    "line": "bunx svgr --icon --title-prop --replace-attr-values '#000=currentColor' -d src/Line assets/Line",
    "duotone": "bunx svgr --icon --title-prop --replace-attr-values '#000=currentColor' -d src/Duotone assets/Duotone",
    "fill": "bunx svgr --icon --title-prop --replace-attr-values '#000=currentColor' -d src/Fill assets/Fill",
    "gen:types": "bun scripts/generate-types.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SargamDesign/sargam-icons-react.git"
  },
  "keywords": [
    "icons",
    "line-icons",
    "fill-icons",
    "iconset",
    "svg",
    "react",
    "library"
  ],
  "author": "@planetabhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SargamDesign/sargam-icons-react/issues"
  },
  "homepage": "https://sargamicons.com/",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@svgr/cli": "^8.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.4",
    "rollup-plugin-filesize": "^10.0.0",
    "sargam-icons": "^1.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4"
  },
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./line": {
      "import": "./dist/line.mjs",
      "require": "./dist/line.cjs"
    },
    "./duotone": {
      "import": "./dist/duotone.mjs",
      "require": "./dist/duotone.cjs"
    },
    "./fill": {
      "import": "./dist/fill.mjs",
      "require": "./dist/fill.cjs"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.8"
  },
  "types": "dist/index.d.ts"
}
